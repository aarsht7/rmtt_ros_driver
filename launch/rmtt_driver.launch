<?xml version="1.0"?>

<launch>
  <arg name="ip" default="192.168.10.2" />
  <node name="rmtt_driver" pkg="rmtt_ros_driver" type="rmtt_driver_node.py" output="screen">
    <param name="IP_ADRESS_STR" value="$(arg ip)"/>
    <rosparam command="load" file="$(find rmtt_ros_driver)/config/rmtt_driver_parameters.yaml" />
  </node>
</launch>
